List<String> getLabels() {
  return ["Mon", "Tue", "Wed", "Fri", "Sat", "Sun"];
}

List<int> getFrequency(List<DateTime> process) {
  List<int> frequency = List.filled(7, 0);

  for (var processes in process) {
    int dayOfWeek = processes.weekday;
    frequency[dayOfWeek - 1]++;
  }

  return frequency;
}
